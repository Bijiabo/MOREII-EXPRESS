nav.navbar.navbar-inverse#console-appnav(role="navigation")
    div.container
        div.navbar-header
            button.navbar-toggle(type="button",data-toggle="collapse",data-target="#console-appnav-collapse")
            a.span.fa.fa-navicon#console-sidebar-menubtn(href="javascript:;")
            a.navbar-brand(href="#",style="font-size:14px;") #{appInfo.cnName}
        div.collapse.navbar-collapse#console-appnav-collapse
            ul.nav.navbar-nav
                -if(consoleNav!==undefined)
                    -for navItem in consoleNav
                        -if(navItem.path===consoleNavActive)
                            li.active
                                a(href="#{siteUrl}#{app}/console/#{navItem.path}") #{navItem.name}
                        -else
                            li
                                a(href="#{siteUrl}#{app}/console/#{navItem.path}") #{navItem.name}






